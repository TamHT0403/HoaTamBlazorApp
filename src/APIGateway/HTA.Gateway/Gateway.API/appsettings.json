{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AuthServer": {
    "Authority": "http://localhost:5000",
    "SwaggerClientId": "swagger-client",
    "SwaggerClientSecret": "swagger-secret"
  },
  "Caching": {
    "Redis": {
      "ConnectionString": "localhost:6379",
      "InstanceName": "HTA.Gateway"
    }
  },
  "JwtConfig": {
    "SecretKey": ""
  },
  "Endpoints": {
    "Gateway": "http://localhost:5000",
    "Account": "http://localhost:5001",
    "Product": "http://localhost:5002"
  },
  "ReverseProxy": {
    "Routes": {
      "account-route": {
        "ClusterId": "account-cluster",
        "Match": {
          "Path": "/account/api/v1/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "account/api/v1"
          },
          {
            "PathPrefix": "/api/v1"
          }
        ]
      },
      "product-route": {
        "ClusterId": "product-cluster",
        "Match": {
          "Path": "/product/api/v1/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/product/api/v1"
          },
          {
            "PathPrefix": "/api/v1"
          }
        ]
      }
    },
    "Clusters": {
      "account-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5001"
          }
        }
      },
      "product-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5002"
          }
        }
      }
    }
  }
}